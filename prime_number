#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cout<<"enter a number";
	cin>>n;
   int fact_count=0;
   int i;
   for(i=0;i<=n;i++)
   {
   	if(n%i==0)
   	{
   		fact_count+=1;
	}
   }
   if(fact_count==0)
   {
   	cout<<n<<"is prime";
   }
   else
   {
   	cout<<n<<"is not a prime";
   }
   return 0;
}
